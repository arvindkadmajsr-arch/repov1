{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "arvind29112025adf"
		},
		"ds_employee_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ds_employee_source'"
		},
		"ls_dept_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_dept_source'"
		},
		"ls_sqldb_dest_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sqldb_dest'"
		},
		"ls_sqldb_dest_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "arvind29112025srvr.database.windows.net"
		},
		"ls_sqldb_dest_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "arvind29112025db"
		},
		"ls_sqldb_dest_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "arvind29112025user"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "arvind29112025adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ds_employee_source_connectionString": {
						"value": "[parameters('ds_employee_source_connectionString')]"
					},
					"ls_dept_source_connectionString": {
						"value": "[parameters('ls_dept_source_connectionString')]"
					},
					"ls_sqldb_dest_password": {
						"value": "[parameters('ls_sqldb_dest_password')]"
					},
					"ls_sqldb_dest_properties_typeProperties_server": {
						"value": "[parameters('ls_sqldb_dest_properties_typeProperties_server')]"
					},
					"ls_sqldb_dest_properties_typeProperties_database": {
						"value": "[parameters('ls_sqldb_dest_properties_typeProperties_database')]"
					},
					"ls_sqldb_dest_properties_typeProperties_userName": {
						"value": "[parameters('ls_sqldb_dest_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}